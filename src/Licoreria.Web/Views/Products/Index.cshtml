@model IEnumerable<dynamic>
@{
    ViewData["Title"] = "Catálogo";
}
<h1>Catálogo</h1>
<form method="get" class="mb-3">
    <input name="q" placeholder="Buscar..." />
    <button type="submit">Buscar</button>
</form>
<table>
  <thead><tr><th>Nombre</th><th>Marca</th><th>Volumen</th><th>% Alc</th><th>Precio</th></tr></thead>
  <tbody>
    @foreach (var p in Model) {
      <tr>
        <td>@p.Name</td>
        <td>@p.Brand</td>
        <td>@p.VolumeMl ml</td>
        <td>@p.AlcoholVolumePct %</td>
        <td>$ @p.Price</td>
      </tr>
    }
  </tbody>
</table>
<p>Servicios: Entrega a domicilio, Reservas para eventos, Facturación electrónica (próximamente).</p>
